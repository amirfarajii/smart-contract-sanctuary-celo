        uint256 total,
        uint256 value,
        uint256 fees
    );
    event CyberMarketCleanBid(
        address nftAddress,
        uint256 tokenId
    );

    event CyberMarketTokenPriceChanged(
        address nftAddress,
        uint256 indexed tokenId,
        uint256 price
    );

}

