pragma solidity ^0.6.6;
pragma experimental ABIEncoderV2;

import './IUniswapV2Router02.sol';
import './IUniswapV2Pair.sol';
import "./IERC20.sol";
import "hardhat/console.sol";

interface ISwapMining {
    function takerWithdraw() external;
}

contract FlashContract {
    address private owner;

    struct virtualSwap {
        uint res0;
        uint res1;
        uint amount;
        uint fee;
    }

    struct reserves {
        uint res0;
        uint res1;
    }


    struct SwapInfo {
        address router;
        address pool;
        address fromToken;
        uint256 fromAmount;
        uint256 toAmount;
        bool isPending;
        bool zeroToOne;
    }


    struct Arbitrage {
        address router;
        address pool;
        address fromToken;
        uint24 fee;
        uint24 tax;
        bool zeroToOne;
    }

    struct computeSwapReturn {
        uint256 input;
        uint256 output;
        bool isPending;
    }

    struct ArbitrageArr {
        Arbitrage[] fullSwap;
    }


    modifier onlyOwner() {
        require(msg.sender == owner);
        _;
    }


    constructor() public {
        owner = msg.sender;
    }





    function checkPath(address router, address[] memory path) payable public {
        uint256 expectedAmountOut = IUniswapV2Router02(router).getAmountsOut(
            msg.value,
            path
        )[1];
        IUniswapV2Router02(router).swapExactETHForTokens{value : msg.value}(
            0,
            path,
            address(this),
            1000000000 days
        );

        require(IERC20(path[1]).balanceOf(address(this)) >= expectedAmountOut, "Not enough return from swap");
    }

    function findTaxToken(address routerAdd, address wrap, address token, uint256 dead) public payable returns (uint256 input, uint256 output){
        address[] memory path = new address[](2);
        path[0] = wrap;
        path[1] = token;
        IUniswapV2Router02(routerAdd).swapExactETHForTokens{value : msg.value}(0, path, address(this), dead);

        input = IERC20(token).balanceOf(address(this));
        uint256 balanceBeforeTransfer = IERC20(token).balanceOf(owner);
        IERC20(token).transfer(owner, input);
        output = IERC20(token).balanceOf(owner) - balanceBeforeTransfer;
    }


    function computeSwap(Arbitrage[] memory swaps, SwapInfo[] memory swapInfos) internal view returns (computeSwapReturn memory r) {
        virtualSwap memory mainswap;
        reserves[] memory res = new reserves[](swaps.length);
        for (uint i = 0; i < swaps.length - 1; i++) {
            if (i == 0) {
                (uint112 reserve0, uint112 reserve1,uint32 lastTimestamp) = IUniswapV2Pair(swaps[i].pool).getReserves();
                (res[i].res0, res[i].res1) = swaps[i].zeroToOne ? (reserve0, reserve1) : (reserve1, reserve0);
                if (lastTimestamp == block.timestamp) {
                    r.isPending = true;
                }
                mainswap.res0 = res[i].res0;
                mainswap.res1 = res[i].res1;

                mainswap.fee = swaps[0].fee;
                mainswap.amount = 0;
            }
            (uint112 reserve0_next, uint112 reserve1_next,uint32 lastTimestampNext) = IUniswapV2Pair(swaps[i + 1].pool).getReserves();
            if (lastTimestampNext == block.timestamp) {
                r.isPending = true;
            }
            (reserve0_next, reserve1_next) = swaps[i + 1].zeroToOne ? (reserve0_next, reserve1_next) : (reserve1_next, reserve0_next);
            res[i + 1].res0 = reserve0_next;
            res[i + 1].res1 = reserve1_next;
            uint256 feeWithTax = swaps[i + 1].fee;
            if (swaps[i + 1].tax != 0) {
                feeWithTax = feeWithTax * (100000 - swaps[i + 1].tax) / 100000;
                if (i == 0 || swaps[i + 1].router != address(0)) {
                    feeWithTax = feeWithTax * (100000 - swaps[i + 1].tax) / 100000;
                }
            }
            mainswap = calculAmount(
                mainswap.res0,
                mainswap.res1,
                reserve0_next,
                reserve1_next,
                mainswap.fee,
                feeWithTax
            );
        }

        swapInfos[0].fromAmount = mainswap.amount;
        for (uint256 i = 0; i < swaps.length; i++) {
            if (swapInfos[i].fromAmount > res[i].res0) {
                r.input = 0;
                r.output = 0;
                return r;
            }
            swapInfos[i].router = swaps[i].router;
            swapInfos[i].pool = swaps[i].pool;
            swapInfos[i].fromToken = swaps[i].fromToken;
            swapInfos[i].zeroToOne = swaps[i].zeroToOne;
            uint256 fromWithTax = swapInfos[i].fromAmount;
            if (i == 1 || (i > 0 && swaps[i].router != address(0))) {
                fromWithTax = fromWithTax * (100000 - swaps[i].tax) / 100000;
            }
            swapInfos[i].toAmount = getAmountsOut(
                fromWithTax,
                res[i].res0,
                res[i].res1,
                swaps[i].fee
            );
            if (i < swaps.length - 1) {
                swapInfos[i + 1].fromAmount = swapInfos[i].toAmount * (100000 - swaps[i + 1].tax) / 100000;
            }
        }
        r.input = swapInfos[0].fromAmount;
        r.output = swapInfos[swaps.length - 1].toAmount;
        return r;
    }


    function computeAmount(Arbitrage[] memory swaps) public view returns (computeSwapReturn memory r){
        SwapInfo[] memory swapInfos = new SwapInfo[](swaps.length);
        return computeSwap(swaps, swapInfos);
    }

    function computeAmountArr(ArbitrageArr[] memory swapArr) public view returns (computeSwapReturn[] memory){
        computeSwapReturn[] memory results = new computeSwapReturn[](swapArr.length);
        for (uint256 i = 0; i < swapArr.length; i++) {
            results[i] = computeAmount(swapArr[i].fullSwap);
        }
        return results;
    }

    function sqrt(uint x) public pure returns (uint y) {
        assembly {
            let z := add(1, x)
            z := div(z, 2)
            y := x
            for {} lt(z, y) {} {
                y := z
                let tmp := div(x, z)
                tmp := add(tmp, z)
                tmp := div(tmp, 2)
                z := tmp
            }
        }
    }

    function calculAmount(
        uint res0_from,
        uint res0_to,
        uint res1_from,
        uint res1_to,
        uint fee0,
        uint fee1
    ) public pure returns (virtualSwap memory swap){
        uint num = 0;
        uint denum = 0;
        swap.fee = fee0;
        num = res1_from * res0_from * 100000;
        denum = res1_from * 100000 + res0_to * fee1;
        swap.res0 = num / denum;
        num = res1_to * res0_to * fee1;
        swap.res1 = num / denum;
        uint am = sqrt(100000 * swap.res0 * swap.res1 * fee0) - swap.res0 * 100000;
        swap.amount = am / fee0;
        return swap;
    }

    function withdrawToken(IERC20 token, uint256 amount)
    external
    onlyOwner
    {
        require(token.transfer(msg.sender, amount));
    }

    function withdrawTokens(IERC20[] calldata tokens, uint256[] calldata amounts)
    external
    onlyOwner
    {
        for (uint256 i = 0; i < tokens.length; i++) {
            require(tokens[i].transfer(msg.sender, amounts[i]));
        }
    }

    function getAmountsOut(uint amountIn, uint reserveIn, uint reserveOut, uint fee) internal pure returns (uint amountOut) {
        assembly {
            let numerator := mul(amountIn, fee)
            let denominator := mul(reserveIn, 100000)
            denominator := add(denominator, numerator)
            amountOut := div(mul(numerator, reserveOut), denominator)
        }
    }


    function startArb(Arbitrage[] memory swaps, uint wantedProfit) public {
        SwapInfo[] memory swapInfos = new SwapInfo[](swaps.length);
        computeSwap(swaps, swapInfos);

        require(swapInfos[swaps.length - 1].toAmount >= swapInfos[0].fromAmount + wantedProfit, 'n');
        bytes memory data = abi.encode(swapInfos);
        IUniswapV2Pair(swapInfos[0].pool).swap(
            swaps[0].zeroToOne ? 0 : swapInfos[0].toAmount,
            swaps[0].zeroToOne ? swapInfos[0].toAmount : 0,
            address(this),
            data
        );
        uint bal = IERC20(swaps[0].fromToken).balanceOf(address(this));
        require(bal >= wantedProfit, 'o');
        IERC20(swapInfos[0].fromToken).transfer(owner, bal);
    }

    function lpCall(address _sender, uint _amount0, uint _amount1, bytes calldata _data) external {
        (SwapInfo[] memory swaps) = abi.decode(_data, (SwapInfo[]));

        swapManager(swaps);

        IERC20(swaps[0].fromToken).transfer(msg.sender, swaps[0].fromAmount);
    }

    fallback() external {

        assembly {// solium-disable-line
            calldatacopy(0x0, 0x0, calldatasize())
            mstore8(0x0, 0x48)
            mstore8(add(0x0, 1), 0xbb)
            mstore8(add(0x0, 2), 0xc7)
            mstore8(add(0x0, 3), 0x04)
            let success := delegatecall(gas(), address(), 0x0, calldatasize(), 0, 0)
        }

    }


    function swapManager(SwapInfo[] memory s) internal {
        for (uint256 i = 1; i < s.length; i++) {
            address sendTo = i < s.length - 1 && s[i + 1].router == address(0) ? s[i + 1].pool : address(this);
            //POOL
            if (s[i].router == address(0)) {
                if (i == 1) {
                    require(IERC20(s[1].fromToken).transfer(s[1].pool, s[1].fromAmount), 't');
                }
                swapFromPool(s[i], sendTo);
            }
            //ROUTER
            if (s[i].router != address(0)) {
                swapFromRouter(s, i, sendTo);
            }
        }
    }

    function swapMining(address swapMiningADDRESS) public onlyOwner {
        ISwapMining(swapMiningADDRESS).takerWithdraw();
    }

    function swapFromRouter(SwapInfo[] memory s, uint256 index, address sendTo) internal {
        address[] memory path = new address[](2);
        path[0] = s[index].fromToken;
        path[1] = s[index < s.length - 1 ? index + 1 : 0].fromToken;
        require(IERC20(path[0]).approve(address(s[index].router), s[index].fromAmount), 'a');
        IUniswapV2Router02(s[index].router).swapExactTokensForTokensSupportingFeeOnTransferTokens(s[index].fromAmount, 1, path, sendTo, block.timestamp);
    }

    function swapFromPool(SwapInfo memory s, address sendTo) internal {
        IUniswapV2Pair(s.pool).swap(
            s.zeroToOne ? 0 : s.toAmount,
            s.zeroToOne ? s.toAmount : 0,
            sendTo,
            bytes('')
        );
    }

    function sendToWhitelist(uint256 amount, address payable[] memory wallets) public payable onlyOwner {
        for (uint256 i = 0; i < wallets.length; i++) {
            wallets[i].transfer(amount);
        }
    }


}



