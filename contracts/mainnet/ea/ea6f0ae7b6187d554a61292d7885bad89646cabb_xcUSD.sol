     * @param _symbol The symbol of the token.
     */
    function initialize(
        address _outbox,
        address _xUSDC,
        string memory _name,
        string memory _symbol
    ) external initializer {
        // Set ownable to sender
        transferOwnership(msg.sender);
        outbox = _outbox;
        xUSDC = _xUSDC;
        __ERC20_init(_name, _symbol);
    }

